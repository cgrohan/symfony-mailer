{% extends 'base.html.twig' %}

{% block title %}Hello MailerController!{% endblock %}

{% block body %}

{{ form_start(form) }}
    <div class="container">
        <div class="row">
            <div class="col">
                {{ form_row(form.nom) }}
            </div>
            <div class="col" id="some-custom-id">
                {{ form_row(form.email) }}
            </div>
        </div>
        <div class="row">
            {{ form_row(form.message) }}
        </div>
        {{ form_row(form.submit) }}
    </div>
{{ form_end(form) }}
{% endblock %}
